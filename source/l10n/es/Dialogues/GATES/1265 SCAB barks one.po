#. Title = GATES / SCAB barks one
#. Description = a whole bunch of scabs barks for one location
#. Actor = Unemployed Person
msgctxt "Dialogue Text/0x0100008900003448"
msgid "\"We can all work together!\""
msgstr ""
"\"¡Podemos trabajar todos juntos!\"."

#. Title = GATES / SCAB barks one
#. Description = a whole bunch of scabs barks for one location
#. Actor = Scabette
msgctxt "Dialogue Text/0x0100008900003454"
msgid "\"Unions are a scam!\""
msgstr ""
"\"¡Los sindicatos son un fraude!\"."

#. Title = GATES / SCAB barks one
#. Description = a whole bunch of scabs barks for one location
#. Actor = Mother of a Scab
msgctxt "Dialogue Text/0x0100008900003303"
msgid "\"Shit or get off the crapper, assholes!\""
msgstr ""
"\"¡Ni coméis ni dejáis comer, comemierdas!\"."

#. Title = GATES / SCAB barks one
#. Description = a whole bunch of scabs barks for one location
#. Actor = Scab
msgctxt "Dialogue Text/0x010000890000344E"
msgid "\"WOORK WOORK WOORK!\""
msgstr ""
"\"¡TRAA-BAA-JO, TRAA-BAA-JO, TRAA-BAA-JO!\"."

#. Title = GATES / SCAB barks one
#. Description = a whole bunch of scabs barks for one location
#. Actor = Mother of a Scab
msgctxt "Dialogue Text/0x01000089000032FD"
msgid "\"We *just* wanna work!\""
msgstr ""
"\"¡*Dejadnos* trabajar!\"."

#. Title = GATES / SCAB barks one
#. Description = a whole bunch of scabs barks for one location
#. Actor = Probably a Migrant
msgctxt "Dialogue Text/0x0100008900003321"
msgid "\"Work is a basic human right!\""
msgstr ""
"\"¡El trabajo es un derecho humano fundamental!\"."

#. Title = GATES / SCAB barks one
#. Description = a whole bunch of scabs barks for one location
#. Actor = Probably a Migrant
msgctxt "Dialogue Text/0x0100008900003315"
msgid "\"LET US IN!\""
msgstr ""
"\"¡DEJADNOS ENTRAR!\"."

#. Title = GATES / SCAB barks one
#. Description = a whole bunch of scabs barks for one location
#. Actor = Probably a Migrant
msgctxt "Dialogue Text/0x010000890000331B"
msgid "\"We wanna work!\""
msgstr ""
"\"¡Queremos trabajar!\"."

#. Title = GATES / SCAB barks one
#. Description = a whole bunch of scabs barks for one location
#. Actor = Former Coal Miner
msgctxt "Dialogue Text/0x010000890000330F"
msgid "\"I'll work for two!\""
msgstr ""
"\"¡Trabajaré el doble!\"."

#. Title = GATES / SCAB barks one
#. Description = a whole bunch of scabs barks for one location
#. Actor = Mother of a Scab
msgctxt "Dialogue Text/0x010000890000345A"
msgid "\"C'mon, guys! I gotta feed my kids!\""
msgstr ""
"\"¡Va, tíos! ¡Tengo bocas que alimentar!\"."

#. Title = GATES / SCAB barks one
#. Description = a whole bunch of scabs barks for one location
#. Actor = Former Coal Miner
msgctxt "Dialogue Text/0x0100008900003309"
msgid "\"I came all the way from Olduwai to work here!\""
msgstr ""
"\"¡Me he venido desde Olduwai para poder trabajar!\"."

#. Title = GATES / SCAB barks one
#. Description = a whole bunch of scabs barks for one location
#. Actor = Scab
msgctxt "Dialogue Text/0x0100008900003442"
msgid "\"Honest work, honest pay!\""
msgstr ""
"\"¡Trabajo honesto, salario honesto!\"."
