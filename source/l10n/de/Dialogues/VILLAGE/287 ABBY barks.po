#. Title = VILLAGE / ABBY barks
#. Description = Just *please* don't call her, alright?"
#. Actor = Don't Call Abigail
msgctxt "Dialogue Text/0x01000007000199DD"
msgid "\"Abigail!\""
msgstr ""
"„Abigail!“"

#. Title = VILLAGE / ABBY barks
#. Description = Just *please* don't call her, alright?"
#. Actor = Don't Call Abigail
msgctxt "Dialogue Text/0x01000007000199CA"
msgid "*whimpers*"
msgstr ""
"<i>wimmert</i>"

#. Title = VILLAGE / ABBY barks
#. Description = Just *please* don't call her, alright?"
#. Actor = Don't Call Abigail
msgctxt "Dialogue Text/0x01000007000199D0"
msgid "\"Abby? Don't call Abigail!\""
msgstr ""
"„Abby? Nicht Abigail anrufen!“"

#. Title = VILLAGE / ABBY barks
#. Description = Just *please* don't call her, alright?"
#. Actor = Don't Call Abigail
msgctxt "Dialogue Text/0x01000007000199D7"
msgid "\"Just please don't call 'er.\""
msgstr ""
"„Dass du sie mir nur nicht anrufst.“"

#. Title = VILLAGE / ABBY barks
#. Description = Just *please* don't call her, alright?"
#. Actor = Don't Call Abigail
msgctxt "Dialogue Text/0x01000007000199C4"
msgid "\"Don't 'ya dare to call' er...\""
msgstr ""
"„Wag es nicht, sie anzurufen …“"

#. Title = VILLAGE / ABBY barks
#. Description = Just *please* don't call her, alright?"
#. Actor = Don't Call Abigail
msgctxt "Dialogue Text/0x0100000700019A5E"
msgid "\"Shit, Abigail... please don't call 'er.\""
msgstr ""
"„Mann, Abigail … Bitte nicht bei ihr anrufen.“"

#. Title = VILLAGE / ABBY barks
#. Description = Just *please* don't call her, alright?"
#. Actor = Don't Call Abigail
msgctxt "Dialogue Text/0x01000007000199E7"
msgid "\"Don't call Abigail.\""
msgstr ""
"„Nicht Abigail anrufen.“"

#. Title = VILLAGE / ABBY barks
#. Description = Just *please* don't call her, alright?"
#. Actor = Don't Call Abigail
msgctxt "Dialogue Text/0x0100000700019A65"
msgid "\"Ar'ya... callin' Abigail?\""
msgstr ""
"„Rufst du etwa … Abigail an?“"
