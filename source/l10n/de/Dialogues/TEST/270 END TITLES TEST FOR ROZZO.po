msgctxt "Dialogue Text/0x010000060001FDDC"
msgid ""
"\"Torson?\"\n"
"\"Yes.\"\n"
"\"McLaine?\"\n"
"\"Yes.\"\n"
"\"Heidelstam?\"\n"
"\"No.\"\n"
"\"Vicquemare?\"\n"
"\"Yes.\"\n"
"\"Du Bois?\"\n"
"\"Yes.\""
msgstr ""
"„Torson?“\n"
"„Ja.“\n"
"„McLaine?“\n"
"„Ja.“\n"
"„Heidelstam?“\n"
"„Nein.“\n"
"„Vicquemare?“\n"
"„Ja.“\n"
"„Du Bois?“\n"
"„Natürlich."

