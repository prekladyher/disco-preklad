#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x0100002B00006C0A"
msgid "\"Stepankedra, 9° centigrade, anafront.\""
msgstr ""
"„Stepankedra, 9° Celsius, Anafront.“"

#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x0100002B000066F3"
msgid "\"La Parbat, 26° centigrade, arid.\""
msgstr ""
"„La Parbat, 26° Celsius, trocken.“"

#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x0100002B00006734"
msgid "\"Vaasa, -12° centigrade, hail.\""
msgstr ""
"„Vaasa, -12° Celsius, Hagel.“"

#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x010000450000FBAF"
msgid "\"Mirova, 20° centigrade, partial cloud cover."
msgstr ""
"„Mirowa, 20° Celsius, teilweise bewölkt.“"

#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x0100002B00006724"
msgid "\"Shestaprel, -9° centigrade, overcast.\""
msgstr ""
"„Schestaprel, -9° Celsius, bewölkt.“"

#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x0100002B000066FF"
msgid "\"Deora, 19° centigrade, clear.\""
msgstr ""
"„Deora, 19° Celsius, klar.“"

#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x0100002B00006740"
msgid "\"Yelinka, -44° centigrade, snowstorm.\""
msgstr ""
"„Jelinka, -44° Celsius, Schneesturm.“"

#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x0100002B00006CD3"
msgid "\"Ilal-Sahrava, 52° centigrade, sun breaks and starbursts.\""
msgstr ""
"„Ilal-Sahrava, 52° Celsius, plötzlicher Sonnenschein und Sternenausbrüche.“"

#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x0100002B0000673A"
msgid "\"Seol Cité, not available.\""
msgstr ""
"„Seol Cité, keine Daten.“"

#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x0100002B000066F9"
msgid "\"Vredefort, 15° centigrade, dull.\""
msgstr ""
"„Vredefort, 15° Celsius, trüb.“"

#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x0100002B00006718"
msgid "\"Loos, 11° centigrade, rainfall.\""
msgstr ""
"„Loos, 11° Celsius, Regen.“"

#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x0100002B0000672A"
msgid "\"Igaunas, -4° centigrade, bad road conditions.\""
msgstr ""
"„Igaunas, -4° Celsius, schlechte Straßenverhältnisse.“"

#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x0100002B0000671E"
msgid "\"Revachol (Zone of Control), 0° centigrade, black ice warning.\""
msgstr ""
"„Revachol (Kontrollierte Zone), 0° Celsius, Glatteisgefahr.“"

#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x0100002B00006705"
msgid "\"Thylakos-by-Pisantique, 28° centigrade, limited rainfall.\""
msgstr ""
"„Thylakos am Pisantik, 28° Celsius, leichter Regen.“"

#. Title = WHIRLING F2 / RADIO barks
#. Description = A radio tuned to an interisolar weather-station.
#. Actor = Radio
msgctxt "Dialogue Text/0x0100002B00006746"
msgid "\"Lohdu, Uuhu Reservation, -22° centigrade, snowdrifts.\""
msgstr ""
"„Lohdu, Uuhu-Reservat, -22° Celsius, Schneeverwehungen.“"
