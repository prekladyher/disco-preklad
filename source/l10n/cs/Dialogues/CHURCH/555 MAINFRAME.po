msgid ""
msgstr ""
"Content-Transfer-Encoding: 8bit\n"
"Content-Type: text/plain; charset=UTF-8\n"
"Language: en"

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000276"
msgid "\"We should probably let it be. This little 'foray' bears little connection to the case, no?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x01000005000002D3"
msgid "The lieutenant says nothing. You see the machine's glowing frame reflected back from his diamond-shaped glasses. You're free to proceed."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Shivers
msgctxt "Dialogue Text/0x0100000500000385"
msgid "On an island on the River Esperance, a small woman, all skin and bones, sits in a room filled with audio equipment. Thousands of tiny lights are reflected back from her prescription lenses, like stars in the dark."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000050000042B"
msgid "\"I don't know the password.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x01000005000004DE"
msgid "\"What do I get out of it?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000B43"
msgid "Let go of the printout."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000B51"
msgid "\"Do you think this log might be connected to the case?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000B61"
msgid "\"6th FEB, '51: Had a little chat with the local fishermen. Said I shouldn't go near that place, that the church was *spooky* and ridden with *narcotics*. It's a little spooky, alright. Still haven't figured out the electricity.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000BA8"
msgid "\"I doubt that we can find any. It's just idle fishermen's gossip to scare away the kids.\" He looks at the stained glass window."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000C03"
msgid "Read the fifth entry."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000CF4"
msgid "\"She must be describing Acele.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000D31"
msgid "Read the tenth entry."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000D92"
msgid "The lieutenant doesn't answer. He follows your gaze, studying the basins. The water shines in them. No ripples."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x01000021000021A8"
msgid "\"Anything else I can help you with?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100007500000032"
msgid "\"I can assure you I'm an expert circuit-bender. I'm not breaking into your computer, I'm using it to access Coalition military datalinks.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000050000028C"
msgid "\"You're right, let's get out of here.\" [Leave.]"
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Encyclopedia
msgctxt "Dialogue Text/0x01000005000002E7"
msgid "The Rehm Prefect is the governmental version of the commonly used Rehm Civic. Although mostly based on the same technology, the Rehm Prefect is equipped with better noise attenuation circuits."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x01000005000003C4"
msgid "\"Now please tell me why you're calling, Fortress Accident.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x01000005000004E5"
msgid "\"After you've told me the filament's name and password you can access its contents.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000AB3"
msgid "\"Sorry, I forgot to look inside the core to check what filament is inside.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Interfacing
msgctxt "Dialogue Text/0x0100005700019505"
msgid "Another filament memory. Press PLAY to talk with the repeater."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100005700019A86"
msgid "The machine's keyboard is still illuminated, revealing virescent PLAY and PRINT buttons."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Soona, the Programmer
msgctxt "Dialogue Text/0x0100005700023C1B"
msgid "\"You won't find any answers from here.\" She barely looks up from the keyboard. You hear the machine whir back to life."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x01000005000002C8"
msgid "You see virescent PLAY and PRINT buttons on the keyboard. A hatch connected to the central compartment is wide open."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Inland Empire
msgctxt "Dialogue Text/0x0100000500000304"
msgid "Better not touch it. Who knows what evils it may hide."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000312"
msgid "Step behind the machine."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000336"
msgid "A bar of fabric right above the keyboard starts producing a soft hum. The sound of static seeps through the machine's planar magnetic driver."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x01000005000003E2"
msgid "\"Yes.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x01000005000003F7"
msgid "The old lady sighs, as a wave of static distorts her voice to a robotic sprawl. \"If you are unsure, could you please take a look?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x01000005000004F0"
msgid "\"Now please tell me if there's anything else I can do for you, Fortress Accident.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000B2A"
msgid "The lieutenant leans closer, scouring the printout over your shoulder. Just as you finish reading he looks up, muttering under his breath:"
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000C6D"
msgid "Read the sixth entry."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000CEB"
msgid "Read the ninth entry."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Interfacing
msgctxt "Dialogue Text/0x0100002100002ACB"
msgid "For setting up her radiocomputer."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100005700019325"
msgid "Read the printout."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Conceptualization
msgctxt "Dialogue Text/0x01000057000193D6"
msgid "That's not bad."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000570001983F"
msgid "\"I'm here on a side case, representing certain music venue organisers. I'm with the RCM, you see.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Alternate1/0x010000570001983F"
msgid "\"We're here on a side case, representing certain music venue organisers.\" "
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x01000005000002A1"
msgid "And before you have time to ask: \"No, I don't know how to operate it or what it's used for. Let's go.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Encyclopedia
msgctxt "Dialogue Text/0x01000005000002B4"
msgid "The Rehm Prefect is the governmental version of the commonly used Rehm Civic model. Although mostly based on the same technology, the Rehm Prefect is equipped with better noise attenuation circuits."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x01000005000003B5"
msgid "\"They're merely cautious,\" says the old lady. \"It's my job to protect their filaments as a password repeater at the East-Insulindian Station.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000B76"
msgid "Read the third entry."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000BC1"
msgid "Read the fourth entry."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000CB2"
msgid "Read the seventh entry."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100002100002122"
msgid "\"Yes, but this machine looks just like the one in Doomed Commercial Area.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100002100002128"
msgid "\"It's also quite similar to the one we have down at the station. Must be the same model.\" He inspects the machine's framework, careful not to touch anything."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Logic
msgctxt "Dialogue Text/0x01000021000021A1"
msgid "Saint-Brune -- that's the church; Rue de Saint-Ghislaine -- the Doomed Commercial Area."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100005700019244"
msgid "Press PRINT."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000750000004A"
msgid "\"Yes, I could -- I'm a cybernetic god. I'm using the network to commandeer Coalition Warship Archer. I will turn its cannons against its masters.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Alternate1/0x010000750000004A"
msgid "\"Fine, I'm actually trying to reach Coalition Warship Archer. I'm on a mission of great responsibility.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Alternate2/0x010000750000004A"
msgid "\"Fine, I'm actually using your Rehm Prefect to reach Coalition Warship Archer. I'm on a mission of great responsibility.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Alternate3/0x010000750000004A"
msgid "\"Yes, I could -- I'm a cybernetic god. I'm using your Rehm Prefect to commandeer Coalition Warship Archer. I will turn its cannons against its masters.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x01000005000004F6"
msgid "\"Yes, I am alive. I am 74 years old and my name is Yvonne.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x0100000500000593"
msgid "\"One moment...\" You hear her flip through a catalogue, before she reads out with studious care:"
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000C88"
msgid "\"MARCH, '51: Some kind of young *discomen* have appeared next to the church. I've been trying to record the silence to find the epicentre, but now it turns out I've also been capturing the future of dance music, one neo-disco song over and over again...\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000CD5"
msgid "Read the eighth entry."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Reaction Speed
msgctxt "Dialogue Text/0x0100000500000E50"
msgid "See -- *ridden* with narcotics. Just like Andre said."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Half Light
msgctxt "Dialogue Text/0x010000210000206D"
msgid "That sounds bad -- a log-in *attempt*. Something a criminal would do."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Half Light
msgctxt "Dialogue Text/0x0100005700019579"
msgid "Oh no... it was already glowing and now it's also making a *sound*? It's probably some alien Seolite technology..."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Soona, the Programmer
msgctxt "Dialogue Text/0x0100007500000079"
msgid "\"Now please, give me some room. I need two seconds to see that you haven't destroyed anything.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x010000050000023E"
msgid "\"Another radiocomputer,\" says the lieutenant, stepping closer. \"And this time it's already turned on.\" He seems cautious around the machine."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x010000050000037E"
msgid "\"As for me, well...\" Some static. Then... \"I am sitting in my cubicle surrounded by a wall of radios.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000397"
msgid "\"Yvonne, my partner here tells me that you're here because radiocomputer guys are all paranoid.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000425"
msgid "\"Is it my birthday?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x010000050000043C"
msgid "\"Still no.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000B3C"
msgid "Read the second entry."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000B4B"
msgid "\"Fourth of February? That's over a month ago... Whoever set up those machines has been here for quite a while.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x010000210000219A"
msgid "\"Saint-Brune -- that's the church,\" the lieutenant gestures around him, \"and Rue de Saint-Ghislaine, that's the Doomed Commercial Area.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x010000570001931F"
msgid "The printer prints out a long text document with dated paragraphs. It looks a bit like someone's journal."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Logic
msgctxt "Dialogue Text/0x01000057000195FF"
msgid "Fortress Accident, just like in the Doomed Commercial Area... There must be a connection."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Soona, the Programmer
msgctxt "Dialogue Text/0x0100005700019739"
msgid "\"Breaking into my radiocomputer, I see.\" She glares at you as she holds down the OFF button for several seconds. The machine reboots."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x010000050000036A"
msgid "\"The filament you have inserted into the core.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x01000005000003E7"
msgid "\"You mean that glowing thing with a tape on it that said \"LOG (FEB-MAR)\"?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x01000005000003FE"
msgid "\"Good. Please repeat the password.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000050000041F"
msgid "\"A password? I'm really bad at passwords. Can you give me a hint?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x0100000500000434"
msgid "\"No.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x0100000500000463"
msgid "\"Fortress Accident, is there anything else I can do for you today?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000523"
msgid "\"Yvonne, it's me again! How are you?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000530"
msgid "\"I looked inside the core, but the tape on the filament just said \"LOG (FEB-MAR)\".\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x0100000500000585"
msgid "\"The company on whose name the terminal you're currently using has been registered to.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000756"
msgid "Let it go."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000CC3"
msgid "\"The pillar of silence! She *is* talking about the silence... Is she suggesting it's more than just an architectural quirk?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000D0F"
msgid "\"MARCH '51: I got a call from the repeater station. Someone has tried to access the radiocomputer in our old office in Martinaise. Can't do anything about it; the storekeeper still doesn't want to let me inside the building. Thinks I'm part of some kind of *curse*. How Martinaise of her.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000D81"
msgid "\"Is she talking about...?\" The lieutenant looks to his right, toward the silence."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100001F0000045A"
msgid "\"Something about the backup data getting destroyed... and how everyone thought it was the author's fault.\" He adjusts his glasses. \"Let's just keep reading.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Reaction Speed
msgctxt "Dialogue Text/0x0100002100002AFA"
msgid "That discoman? Must be Acele."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x010000570001974C"
msgid "\"Psst, someone's coming...\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x010000570001D3DC"
msgid "\"Good morning, Fortress Accident on Saint-Brune, this is the East-Insulindian Repeater Station 1. Please repeat, is this the personal log?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000750000003E"
msgid "\"Okay, I'm actually trying to locate a suspect in a murder investigation. I thought they might be hiding here. I'm with the RCM, you see.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Esprit de Corps
msgctxt "Dialogue Text/0x0100000500000254"
msgid "These machines sometimes harbour traps, he thinks -- alarm systems and the like. Let's be careful."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000286"
msgid "\"Wait, let me just investigate it...\" (Step behind the computer.)"
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000370"
msgid "\"Okay, but where are you? How did you know where I am?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x01000005000004B6"
msgid "\"It just says 'LOG (FEB-MAR)' and nothing else.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Empathy
msgctxt "Dialogue Text/0x010000050000073D"
msgid "Doesn't it get lonely, sitting there all by herself?"
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Electrochemistry
msgctxt "Dialogue Text/0x0100000500000B88"
msgid "What was that about narcotics? This could prove to be interesting."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000C7C"
msgid "\"28th FEB, '51: Yes, the first recordings confirm that the swallow is real and I'm not just losing my mind. It's a pillar of silence with a diameter of approximately three meters. Seems like the higher I go, the less I record. This might be a coincidence. Or it *could* be connected to the data loss that led me here.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000D6A"
msgid "\"Why would anyone need *any* of this equipment here?\" He steps on a wire running on the ground, inspecting it with his boot."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x01000021000019CC"
msgid "Press PLAY again."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000210000214D"
msgid "\"Fortress Accident... like the one in the Doomed Commercial Area?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x0100002100002167"
msgid "\"I have two machines registered to this company name in Martinaise -- one on Saint-Brune, the other on Rue de Saint-Ghislaine.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Soona, the Programmer
msgctxt "Dialogue Text/0x010000750000006D"
msgid "\"No one asked you to do anything *interesting* with my radiocomputer.\" She barely looks up from the keyboard. You hear the machine whir back to life."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Interfacing
msgctxt "Dialogue Text/0x01000005000002DC"
msgid "This is the Rehm Prefect, radiocomputer model number RC7024, equipped with a Feld mainframe and a Rehm-compatible Interim printer."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000050000046F"
msgid "\"How do I do that?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x0100000500000549"
msgid "\"Good, I've unlocked the filament. After ending the call please press PRINT to access the filament.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000750"
msgid "\"Doesn't it get lonely, doing this job?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000C26"
msgid "\"Her friends, colleagues?\" He shrugs. \"She must be quite educated if she knew how to set up all this machinery.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Conceptualization
msgctxt "Dialogue Text/0x010000210000213A"
msgid "A quaint little box of radio waves."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000570001924A"
msgid "[Leave.]"
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Interfacing
msgctxt "Dialogue Text/0x0100005700019530"
msgid "This is the machine's filament memory. Press PLAY to access its contents."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100005700023C0B"
msgid "\"I'm not *just* breaking in! I'm pursuing a mysterious lead, in search of my lost identity.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Alternate1/0x0100005700023C0B"
msgid "\"We're not just breaking in! I'm pursuing a mysterious lead, searching for my lost identity.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000232"
msgid "A machine stands in the corner, watched over by the figures on the stained glass window. It's turned on and quivering with soft electricity."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000390"
msgid "The lieutenant whispers into your ear: \"She repeats passwords. Programming people are all... paranoid.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x01000005000005A8"
msgid "\"And what's that -- this 'interactive call-in radio game'?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000C46"
msgid "\"12th FEB, '51: Brought some food from the grocery store. Apparently there's a strike going on in the harbour. Definitely not happy to see the Martinaise people again. Everything's now set up in the church, going to start working tomorrow 8 AM.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x010000570001933E"
msgid "The speaker comes to life, static seeps through the machine's planar magnetic driver. An old lady greets you, her voice sounds a hundred years old..."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100005700019815"
msgid "\"I do apologize for the intrusion, madam. We're with the RCM, you see.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x010000050000025C"
msgid "\"We should probably let it be. It's becoming clear that there's no trace of the suspect anywhere inside the church.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Interfacing
msgctxt "Dialogue Text/0x01000005000002A8"
msgid "This is the Rehm Prefect radiocomputer, a model number RC7024, equipped with a Feld mainframe and a Rehm-compatible Interim printer."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x01000005000003DB"
msgid "\"I don't know, maybe?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x0100000500000572"
msgid "\"Now, can you please repeat the password?\" the voice in the machine asks again. She sounds cold in the damp air."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000651"
msgid "Machine's keyboard is still illuminated, revealing virescent PLAY and PRINT buttons."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000D9A"
msgid "\"That's me. I was the one who broke into that radiocomputer! And the storekeeper must be Plaisance...\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000E48"
msgid "\"Arrived at the church. The door was boarded up, so I used a crowbar to get inside. Looks like the place has been deserted. Nothing out of the ordinary, but I'll ask around. Need to figure out how to get the electricity in.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x01000021000016EF"
msgid "\"I wouldn't know, I'm only here for passwords.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100005700019337"
msgid "Press PLAY."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Soona, the Programmer
msgctxt "Dialogue Text/0x0100005700019716"
msgid "A strange woman makes straight for the radiocomputer."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Perception
msgctxt "Dialogue Text/0x0100005700019747"
msgid "Your reading is interrupted by the sound of the church door opening."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000570001985E"
msgid "\"I'm looking for a suspect in a murder investigation. I thought they might be hiding here. I'm with the RCM, you see.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Alternate1/0x010000570001985E"
msgid "\"We're looking for a suspect in a murder investigation. We thought they might be hiding here.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Soona, the Programmer
msgctxt "Dialogue Text/0x0100005700023C23"
msgid "\"Well, you won't find any music venue organisers here.\" She barely looks up from the keyboard. You hear the machine whir back to life."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Interfacing
msgctxt "Dialogue Text/0x010000750000002C"
msgid "Yes, you *are* breaking in. But not into *her* radiocomputer. You're a master circuit-bender."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Soona, the Programmer
msgctxt "Dialogue Text/0x0100007500000038"
msgid "\"There are no *Coalition military datalinks*.\" She barely looks up from the keyboard. \"And even if there was, you couldn't access them over a civilian network.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100007500000050"
msgid "\"You're right, I'm just a drunk cop. I don't know how to do anything interesting with radiocomputers.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Soona, the Programmer
msgctxt "Dialogue Text/0x0100007500000066"
msgid "\"That's nonsense. You're just a police officer.\" She barely looks up from the keyboard. You hear the machine whir back to life."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Conceptualization
msgctxt "Dialogue Text/0x01000005000002BC"
msgid "A quaint little box of radio waves."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x01000005000003CD"
msgid "\"Ah, right. Thanks for the explanation.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x01000005000005B0"
msgid "The static drowns her response. \"Any other questions?\" you hear her ask when the connection finally improves."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000CFB"
msgid "\"The girl on the ice? Sounds like her, yes.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Interfacing
msgctxt "Dialogue Text/0x010000060001E4D8"
msgid "No-no. It was a great idea. You're learning things. This is how you learn things about machines."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Interfacing
msgctxt "Dialogue Text/0x01000021000016DF"
msgid "This is a *human interface* -- a retired person sitting behind a switchboard somewhere."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Empathy
msgctxt "Dialogue Text/0x01000021000016F8"
msgid "That's why she does this."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Soona, the Programmer
msgctxt "Dialogue Text/0x010000750000001C"
msgid "You're so engrossed in your reading that you don't even notice the sound of the door opening..."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000555"
msgid "\"This is the police. Please open this thing!\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x010000050000055E"
msgid "The voice recites: \"I am contractually obliged to protect the privacy of the filament holder Fortress Accident. Without filing a warrant with Lintel I cannot give you access to this filament.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000600"
msgid "\"Let's look around. There's no use trying to guess the answer.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Authority
msgctxt "Dialogue Text/0x0100000500000BEE"
msgid "Artists, yet again!! Harassing citizens, stealing badges, 'occupying' public spaces with 'installations' like the one here..."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000C53"
msgid "\"The strike...\" He strokes his chin. \"We're nearing the date of the murder.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000D63"
msgid "\"Why would she need an antenna?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000E58"
msgid "\"Keep reading, I'm interested now -- I want to know what's that radio anomaly that sent this person here in the first place.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Encyclopedia
msgctxt "Dialogue Text/0x0100002100001700"
msgid "A hint system is not part of the protocol for repeater stations."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x010000570001D3E8"
msgid "\"Good evening, Fortress Accident on Saint-Brune, this is the East-Insulindian Repeater Station 1. Please repeat, is this the personal log?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x01000005000003EE"
msgid "\"I swear I haven't touched anything!\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x01000005000004D0"
msgid "\"Alright, I'll do that.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x010000050000050E"
msgid "\"Good bye, Fortress Accident,\" she says as her voice disappears into a whirl of static."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x0100000500000529"
msgid "\"Good, thank you.\" It's not clear whether she recognizes your voice. \"Please repeat, is this the personal log?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x010000050000056B"
msgid "\"I'm afraid we're not doing that. Unless we want to wait for a month.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000050000057F"
msgid "\"What's Fortress Accident?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Alternate1/0x010000050000057F"
msgid "\"Fortress Accident -- sounds familiar...\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000617"
msgid "\"Maybe *he* knows something.\" The lieutenant nods toward Tiago."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x010000050000062E"
msgid "A machine stands in the corner, watched over by the figures on the stained glass window. It's turned on and quivering with soft electricity."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Perception (Hearing)
msgctxt "Dialogue Text/0x01000057000194C4"
msgid "It's the same old woman you spoke with through the radiocomputer in the Doomed Commercial Area."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x010000570001958B"
msgid "The static gets louder, slowly filling up the abandoned hall. Until a voice speaks out, crackling and old, cutting into the air..."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Soona, the Programmer
msgctxt "Dialogue Text/0x0100005700023E0D"
msgid "\"It's just me and my computer -- and it has been this way for weeks. Now please, give me some room. I need two seconds to see that you haven't destroyed anything.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000238"
msgid "\"A radiocomputer,\" says the lieutenant, stepping closer. \"And it's turned on.\" He seems cautious around the machine."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x010000050000047D"
msgid "\"Open the hatch on the compartment. Don't touch the wires, a current is running through it, but the core is safe. There's a filament inside -- I need to know what it's called.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x01000005000005A2"
msgid "\"Wow. So conceptual.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000635"
msgid "The lieutenant doesn't say anything, but you can sense that he doesn't like you meddling with the machine."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000C1F"
msgid "\"Artists, programmers, *Lexie* -- who are all those people?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000C36"
msgid "\"A data loss?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Encyclopedia
msgctxt "Dialogue Text/0x0100002100002140"
msgid "The Rehm Prefect is the governmental version of the commonly used Rehm Civic model. Although mostly based on the same technology, the Rehm Prefect is equipped with better noise attenuation circuits."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Logic
msgctxt "Dialogue Text/0x0100002100002AE1"
msgid "She must be talking about her former colleagues from Fortress Accident. They all sound like *radio game people*."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Shivers
msgctxt "Dialogue Text/0x0100002100002AEB"
msgid "A shiver runs through you as you look to your left, toward the silence."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x010000570001D3E2"
msgid "\"Good afternoon, Fortress Accident on Saint-Brune, this is the East-Insulindian Repeater Station 1. Please repeat, is this the personal log?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x010000050000026F"
msgid "\"We should leave; I doubt this place bears any connection to the case.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Conceptualization
msgctxt "Dialogue Text/0x01000005000002ED"
msgid "A quaint little box of radio waves."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x010000050000035E"
msgid "\"Yes, I am alive. I am 74 years old and my name is Yvonne. Now please repeat, is this the personal log?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x01000005000004D6"
msgid "\"Yes. First you have to end this call though, and then you have to call us back. Is there anything else I could do for you meanwhile?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x01000005000005E0"
msgid "\"Let's try this again. I think I may have the right password for the personal log.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x0100000500000AB9"
msgid "\"Please end the call and do that.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000C3D"
msgid "\"Seems like something to do with radiocomputers -- unfortunately, I don't know enough about them to understand what the author is saying.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000C59"
msgid "\"25th FEB, '51: I've been sending data up to Lintel for a while now, trying to recreate the data loss, but nothing. Didn't even feel like logging in the disappointment. But I did discover a curious *audio-spatial* anomaly at the back of the church. I've named it *the swallow*. (It swallows sound.) Need to get some mics.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x010000060001E4BC"
msgid "\"I think these people worked in the radiocomputer games business. The one we saw in the Doomed Commercial Area. They must be her former co-workers.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000570001923E"
msgid "Look inside the compartment."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x010000570001954D"
msgid "Nothing happens."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x01000005000003D3"
msgid "\"That was a question. Have you inserted it into the core?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x0100000500000508"
msgid "\"Sleep well, Fortress Accident,\" she says as her voice disappears into a whirl of static."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000AA7"
msgid "\"I looked inside the core, but the tape on the filament just said \"LOG (FEB-MAR)\"."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000B59"
msgid "\"Our case? No, I don't think so. It must be some local...\" his eyes wander to the various machines around him, \"*eccentric*.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000B91"
msgid "\"Wait, *narcotics*?\" (Look around the derelict building.)"
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000C9C"
msgid "\"Fortunately, the song is so monotonous I was able to devise an algorithm to factor it out. The other day one of the discomen came in. Before I could even say hello, she got scared and left. Good, I don't want anyone distracting me from my work.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000D8B"
msgid "\"But what could it be?\" (Look at the water basins behind you.)"
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000DA1"
msgid "\"I knew it wasn't a good idea to meddle with that machine...\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100005700019434"
msgid "\"Thanks, but I'm finished with this call.\" (Press OFF/SILENT.)"
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Soona, the Programmer
msgctxt "Dialogue Text/0x010000570001973F"
msgid "\"Breaking into my radiocomputer again, I see.\" She glares at you as she holds down the OFF button for several seconds. The machine reboots."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000294"
msgid "\"Yes, but what is this strange thing?\" (Point at the machine.)"
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x0100000500000405"
msgid "\"I don't know, that's what I'm asking. Is this the personal log?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000050000058C"
msgid "\"Do you have any other information about this company?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x010000050000059A"
msgid "\"'Fortress Accident SCA produces revolutionary interactive call-in radio games' -- that's what the catalogue says.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000D09"
msgid "\"MARCH '51: A new 2m AUX cable, noodles, crackers, Ping-Ping energy drinks, water, TOOTHPASTE, gum, also some canned air...\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Drama
msgctxt "Dialogue Text/0x0100002100000517"
msgid "Then why doth the lieutenant protest against spending time here? So often?"
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Logic
msgctxt "Dialogue Text/0x0100002100002052"
msgid "Password. Of course it would have a password. That's why there's a human administrator involved."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Suggestion
msgctxt "Dialogue Text/0x010000210000205D"
msgid "You should ask her for a hint."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100005700019254"
msgid "Behind the hatch sits a cube-like crisscross of filaments, smouldering in the dark like fireflies. Silver tape on the side says in black marker: \"LOG (FEB-MAR)\"."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Soona, the Programmer
msgctxt "Dialogue Text/0x0100005700019829"
msgid "\"No one's hiding here.\" She barely looks up from the keyboard. You hear the machine whir back to life."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x010000050000029A"
msgid "\"A radiocomputer,\" the lieutenant repeats a little impatiently, \"we have one down at the station.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Reaction Speed
msgctxt "Dialogue Text/0x01000005000002FD"
msgid "Another radiocomputer! Could it bear connection to the one in the Doomed Commercial Area?"
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000318"
msgid "Back off. [Leave.]"
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000326"
msgid "As you step closer, you notice virescent PLAY and PRINT buttons on the keyboard. A hatch connected to the central compartment is wide open."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000050000034A"
msgid "\"What's 'the personal log'?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x0100000500000444"
msgid "\"Received. I will *register* this log-in attempt.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x010000050000053B"
msgid "\"After life death.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000663"
msgid "The first entry, made on the 4th February '51 by an unknown author, is short and concise:"
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000BB0"
msgid "\"Nothing *spooky* about this place either, it's just abandoned. And cold. And in an awful part of town...\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000BFA"
msgid "\"But at least the artists have their act together -- they're qualified labour, they can get work anywhere: graphic design, ads. The programmers are doing fine too, I mean, they're programmers. The writers, though... they're fucked.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = You
msgctxt "Dialogue Text/0x0100000500000343"
msgid "\"What are you, a *machine*? Or are you *alive*?\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x0100000500000377"
msgid "\"I work as a repeater at the East-Insulindian Repeater Station. It's my job to know where you are, Fortress Accident.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = East-Insulindian Repeater Station
msgctxt "Dialogue Text/0x0100000500000748"
msgid "\"Lonely?\" For the first time you hear her chuckle through the rustle of static. \"Why would it get lonely, I get to talk all day to people like you over a warm cup of tea.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Half Light
msgctxt "Dialogue Text/0x0100000500000B82"
msgid "See? Even one of the spookers themselves says it's unnerving."
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000BBB"
msgid "\"7th FEB, '51: Finally got the electricity in! Next on the agenda: a new antenna. I'm thinking ESKER series? Something advanced.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000BC7"
msgid "\"8th FEB, '51: Bought the antenna, had some problems setting it up, called Simo for help. Heard the others are back to *making art* (drinking somewhere out of town). Sulisław started a rock band again, Lexie has been seen asking money from strangers...\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Mainframe
msgctxt "Dialogue Text/0x0100000500000C2E"
msgid "\"I just have to find out what caused that data loss and be done with it. Still don't understand how it managed to wipe out the backup when the backup *wasn't even connected to the front*. I know, I know... everyone thinks it's impossible; they say I must be lying. I'm here to set it right.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Kim Kitsuragi
msgctxt "Dialogue Text/0x0100000500000DBD"
msgid "\"We should talk to her,\" the lieutenant whispers, \"after she has rebooted the machine.\""
msgstr ""

#. Title = CHURCH / MAINFRAME
#. Description = Mainframe. Related to Doomed-Soona-questline related stuff.
#. Actor = Interfacing
msgctxt "Dialogue Text/0x010000210000212F"
msgid "The one you saw earlier was the Rehm Civic -- this is the Rehm *Prefect*, a model number RC7024, equipped with a Feld mainframe and a Rehm-compatible Interim printer."
msgstr ""
